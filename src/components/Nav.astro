---
import Logo from "../img/home/Logo.png";
import Logo2 from "../img/home/Logo1.webp";
---

<nav class="w-full flex-center gap-5 h-[10vh] md:h-[15vh] min-h-[100px]">
  <a class="flex gap-1" href="../">
    <img src={Logo.src} alt="" />

    <img src={Logo2.src} alt="" />
  </a>
  <h1 class="text-2xl md:text-5xl lg:text-7xl text-lw-blue">
    CITY OF <b>LYNWOOD</b>
  </h1>
</nav>

<style>
  img {
    @apply w-[50px] md:w-[70px] lg:w-[100px] object-contain;
  }
</style>
